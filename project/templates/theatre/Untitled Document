<script type="text/javascript" src="https://maps.googleapis.com/maps/api/js?libraries=visualization&sensor=true_or_false&key=AIzaSyDA6qoUKQ3WJQd4ZxAahhFPM80WJbw-yws"></script>
<script type="text/javascript">
	function initialize() {
		var centerlatlng = new google.maps.LatLng(12.750000, 77.250000);
		var myOptions = {
			zoom: 10,
			center: centerlatlng,
			mapTypeId: google.maps.MapTypeId.ROADMAP
		};
		var map = new google.maps.Map(document.getElementById("map_canvas"), myOptions);

		var latlng = new google.maps.LatLng(14.550000, 77.416667);
		var img = new google.maps.MarkerImage('/home/appiness/Desktop/restapi/venv/lib/python3.5/site-packages/gmplot/markers/000000.png');
		var marker = new google.maps.Marker({
		title: "no implementation",
		icon: img,
		position: latlng
		});
		marker.setMap(map);

		var latlng = new google.maps.LatLng(12.979120, 77.591300);
		var img = new google.maps.MarkerImage('/home/appiness/Desktop/restapi/venv/lib/python3.5/site-packages/gmplot/markers/000000.png');
		var marker = new google.maps.Marker({
		title: "no implementation",
		icon: img,
		position: latlng
		});
		marker.setMap(map);

	}
</script>
</head>
<body style="margin:0px; padding:0px;" onload="initialize()">
